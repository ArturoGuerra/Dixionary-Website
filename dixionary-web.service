[Unit]
Description = uWSGI instance to run dixionary.com
After = network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/var/www/dixionary
Enviroment="PATH=/var/www/dixionary/venv/bin"
ExecStart=/var/www/dixionary/venv/bin/gunicorn -b unix:dixionary.sock --reload --access-logfile dixionary.log wsgi:app

[Install]
WantedBy=muti-user.target
